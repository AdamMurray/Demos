{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "rules": {
    "curly": 2,
    "max-len": [
      1,
      100,
      2
    ],
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 1
      }
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ],
    "eqeqeq": [
      "error",
      "always"
    ],
    "no-underscore-dangle": 0,
    "padded-blocks": "off",
    "wrap-iife": "off",
    "no-use-before-define": "off",
    "no-param-reassign": "off",
    "arrow-body-style": "warn",
    "func-names": "off",
    "eol-last": "off"
  },
  "globals": {
    "Polymer": true,
    "THREE": true
  }
}